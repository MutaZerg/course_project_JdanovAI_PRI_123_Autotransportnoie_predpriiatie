@model VehicleCompany.Models.Seat
@{
    ViewData["Title"] = "Seat Details";
}

<h1>Details</h1>
<dl class="row">
    <dt class="col-sm-2">Id</dt>
    <dd class="col-sm-10">@Model.Id</dd>
    <dt class="col-sm-2">Vehicle</dt>
    <dd class="col-sm-10">@(Model.AssignedVehicle?.Vehicle_info ?? Model.AssignedVehicleId.ToString())</dd>
    <dt class="col-sm-2">Booked</dt>
    <dd class="col-sm-10">@(Model.IsBooked ? "Yes" : "No")</dd>
</dl>
<div>
    <a asp-action="Edit" asp-route-id="@Model.Id">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
